<p id="notice"><%= notice %></p>

<h1>Progs</h1>

<table>
  <thead>
    <tr>
      <th>Название</th>
      <th>Лицензия</th>
      <th>Тип лицензии</th>
      <th>Лиценз №</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @progs.each do |prog| %>
      <tr>
        <td><%= prog.Название %></td>
        <td><%= prog.Лицензия %></td>
        <td><%= prog.Тип_лицензии %></td>
        <td><%= prog.Лиценз_№ %></td>
        <td><%= link_to 'Show', prog %></td>
        <td><%= link_to 'Edit', edit_prog_path(prog) %></td>
        <td><%= link_to 'Destroy', prog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prog', new_prog_path %>
