<p id="notice"><%= notice %></p>
<%provide(:title, "Проги")%>
<h1>Progs</h1>

<table style="border:1px solid">
  <thead>
    <tr>
      <% (0..Prog.attribute_names.count-1).each do |i|%>
	<th style="border:1px solid"><%=Prog.attribute_names[i].to_s.capitalize%></th>
      <%end%>
      <th style="border:1px solid">Show</th>
      <th style="border:1px solid">Edit</th>
      <th style="border:1px solid">Destroy</th>
    </tr>
  </thead>

  <tbody>
    <%@progs.each do |prog| %>
      <tr>
	<% (0..Prog.attribute_names.count-1).each do |i|%>
	<td style="border:1px solid"><%=pc.attributes[Prog.attribute_names[i]]%></td>
	<%end%>
        <td style="border:1px solid"><%= link_to 'Show', prog %></td>
        <td style="border:1px solid"><%= link_to 'Edit', edit_prog_path(prog) %></td>
        <td style="border:1px solid"><%= link_to 'Destroy', prog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prog', new_prog_path %>
